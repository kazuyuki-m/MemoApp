{
    "env": {
        // "browser": true,
        // "es2021": true
    },
    "extends": [
        // "eslint:recommended",
        // "plugin:react/recommended"
        "airbnb"
    ],
    // "parserOptions": {
    //     "ecmaFeatures": {
    //         "jsx": true
    //     },
    //     "ecmaVersion": 12,
    //     "sourceType": "module"
    // },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "react/style-prop-object": 0,
        "no-use-before-define": 0,
        "space-before-function-paren": 0,
        "func-names": 0
    },
    "parser": "babel-eslint"
}