{
    "env": {
        // "browser": true,
        // "es2021": true
    },
    "extends": [
        // "eslint:recommended",
        // "plugin:react/recommended"
        "airbnb"
    ],
    // "parserOptions": {
    //     "ecmaFeatures": {
    //         "jsx": true
    //     },
    //     "ecmaVersion": 12,
    //     "sourceType": "module"
    // },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "react/style-prop-object": 0,
        "no-use-before-define": 0,
        "space-before-function-paren": 0,
        "func-names": 0,
        "arrow-body-style": 0,
        "react/prop-types": 0,
        "react/self-closing-comp": 0,
        "no-unused-vars": 0,
        "comma-dangle": 0,
        "semi": 0
    },
    "parser": "babel-eslint"
}